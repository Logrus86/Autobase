<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
        >
    <b:Form ui:field="searchForm" styleName="searchForm">
        <b:Label styleName="searchFormLabel" text="Find vehicle"/>
        <b:Container styleName="searchFormBody">
            <b:TabPanel tabPosition="BELOW">
                <b:TabPane title="Bus">
                    <g:Label>Bus</g:Label>
                    <g:Label>Bus</g:Label>
                </b:TabPane>
                <b:TabPane title="Car" active="true">
                    <g:Label>Car</g:Label>
                    <g:Label>Car</g:Label>
                </b:TabPane>
                <b:TabPane title="Truck">
                    <g:Label>Truck</g:Label>
                    <g:Label>Truck</g:Label>
                </b:TabPane>
            </b:TabPanel>
        </b:Container>
    </b:Form>
</ui:UiBinder>