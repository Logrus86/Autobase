<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
        >
    <b:WellForm ui:field="searchForm" styleName="searchForm">
        <g:Label styleName="searchLabel" text="Find vehicle"/>
        <b:Container styleName="margin20">
            <b:TabPanel tabPosition="above" ui:field="tabPanel">
                <b:TabPane heading="Bus">
                    <g:Label>Bus</g:Label>
                    <g:Label>Bus</g:Label>
                </b:TabPane>
                <b:TabPane heading="Car" active="true">
                    <g:Label>Car</g:Label>
                    <g:Label>Car</g:Label>
                </b:TabPane>
                <b:TabPane heading="Truck">
                    <g:Label>Truck</g:Label>
                    <g:Label>Truck</g:Label>
                </b:TabPane>
            </b:TabPanel>
        </b:Container>
    </b:WellForm>
</ui:UiBinder>